<Application x:Class="Automata.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Automata"
			 xmlns:System="clr-namespace:System;assembly=mscorlib"
	StartupUri="Screens\AutomataSelect.xaml">
	<!-- TODO: change StartupUri to Login.xaml once testing is done-->
    <Application.Resources>
		<SolidColorBrush x:Key="AppBackground">#333333</SolidColorBrush>
		<SolidColorBrush x:Key="SectionBackground">#666666</SolidColorBrush>
		<SolidColorBrush x:Key="TextBackground">#999999</SolidColorBrush>
		<SolidColorBrush x:Key="TextForeground">#B0B0B0</SolidColorBrush>
		<SolidColorBrush x:Key="InnerTextForeground">#FFFFFF</SolidColorBrush>
		<SolidColorBrush x:Key="TitleForeground">#FFA500</SolidColorBrush>
	</Application.Resources>
</Application>
